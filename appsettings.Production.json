{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },

  "Cors": {
    "AllowedOrigins": "${FRONTEND_URL}"
  },

  "Jwt": {
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "Key": "${JWT_SECRET_KEY}"
  },

  "FileStorage": {
    "ExpedientesPath": "Uploads/Expedientes"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },

  "AllowedHosts": "*"
}
